<table class="data-table admin__control-table">
    <thead>
        <tr>
            <th><?= $block->escapeHtml(__('SKU')) ?></th>
            <th><?= $block->escapeHtml(__('Product Name')) ?></th>
            <th><?= $block->escapeHtml(__('Our Price')) ?></th>
            <th><?= $block->escapeHtml(__('Our Cost')) ?></th>
            <th><?= $block->escapeHtml(__('Competitor Price')) ?></th>
        </tr>
    </thead>
    <tbody>
        <?php foreach ($block->getCompetitivePricingProducts() as $product): ?>
